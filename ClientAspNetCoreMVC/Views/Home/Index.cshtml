@model IEnumerable<UserDto>

@{
    ViewData["Title"] = "Home Page";
}

<h1>users</h1>

<table class="table">
    <tr>
        <th>Id</th>
        <th>Name</th>
    </tr>
    @foreach (var item in Model??new List<UserDto>())
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
        </tr>
    }
</table>